package week1.day1;

public class primenumber {

	public static void main(String[] args) {
		int num = 38237882347374827872348728478237482748274239478293742873874587458365346;
		boolean isPrime = true;

		for (int i = 2; i < num; i++)

		{
			if (num % i == 0) {
				isPrime = false;
				break;

			}

		}

		if (isPrime) {
			System.out.println(num + "is a prime");

		} else {
			System.out.println(num + "is not prime");
		}
	}

}
